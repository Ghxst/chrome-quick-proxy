{
    "key": "",
    "name": "Quick Proxy Auth",
    "description": "Set an authenticated proxy, control WebRTC, and import cookies.",
    "version": "1.2.0",
    "manifest_version": 3,
    "permissions": [
        "proxy",
        "cookies",
        "downloads",
        "privacy",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "webRequestAuthProvider"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_title": "Proxy Settings",
        "default_popup": "popup.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    },
    "background": {
        "service_worker": "background.js"
    }
}